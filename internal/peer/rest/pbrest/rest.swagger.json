            "type": "string"
          },
          {
            "name": "chaincode",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "ctor",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "peer",
