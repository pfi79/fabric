            "required": false,
            "type": "boolean"
          },
          {
            "name": "waitForEvent",
            "in": "query",
            "required": false,
            "type": "boolean"
          },
          {
            "name": "waitForEventTimeout",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "Invoke"
        ]
